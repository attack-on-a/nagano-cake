<div class="serch_form">
    <%= form_with url: searchs_search_path(model:"item"),method: :get, local: true do |f| %>
    <%= f.text_field :content, placeholder: "Search" %>
    <%= f.hidden_field :model,  :value => "item" %>
    <%= button_tag type: "submit", class: "btn btn-default" do %>
      <i class="fas fa-search"></i>
      <% end %>
    <% end %>
</div>
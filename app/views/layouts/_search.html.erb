<div class="search_form d-flex justify-content-end">
  <div>
    <%= form_with url: searchs_search_path(model:"item"),method: :get, local: true do |f| %>
      <%= f.text_field :content, class: "form-control", placeholder: "Search" %>
      <%= f.hidden_field :model, :value => "item" %>
  </div>
  <div>
      <%= button_tag type: "submit", class: "btn btn-light" do %>
        <i class="fas fa-search"></i>
      <% end %>
    <% end %>
  </div>
</div>
<table class='table table-borderless bg-white'>
  <thead class="border border-dark">
    <tr>
      <th class="text-center px-0">ジャンル検索</th>
    </tr>
  </thead>
  <tbody class="border border-dark">
    <% genres.each do |genre| %>
      <tr>
        <td class="pr-0">
          <%= link_to genre.name, searchs_search_path(content:genre.name, model:"genre"), class:"text-dark" %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

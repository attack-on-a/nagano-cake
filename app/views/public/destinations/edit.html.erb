<h1>Public::Destinations#edit</h1>
<p>Find me in app/views/public/destinations/edit.html.erb</p>


<h2>配送先編集</h2>

  <%= form_with model: @destinations, url: destinations_path, method: :patch do |f| %>
<table>
  <tr>
    <td>郵便番号(ハイフンなし)</td>
    <td><%= f.text_field :post_code %> </td>
  </tr>
  <tr>
    <td>住所</td>
    <td> <%= f.text_field :address, size: "50*50" %> </td>
  </tr>
  <tr>
    <td>宛名</td>
    <td> <%= f.text_field :name %> </td>
    <td> <%= f.submit '変更内容を変更', calss: "btn btn-success" %> </td>
  </tr>
</table>
<% end %>